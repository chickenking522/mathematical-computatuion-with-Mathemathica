       RowBox[{"x_", " ", "?", "StringQ"}], ",", " ", "k_"}], "]"}], ":=", 
     " ", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"n", ",", "numerictext"}], "}"}], ",", "\[IndentingNewLine]", 
       
       RowBox[{
        RowBox[{"L", "=", 
         RowBox[{"{", "}"}]}], ";", " ", 
        RowBox[{"(*", 
         RowBox[{"empty", " ", "list"}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{"numerictext", "=", 
         RowBox[{
          RowBox[{"ToCharacterCode", "[", 
           RowBox[{"ToUpperCase", "[", "x", "]"}], "]"}], "-", "65"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Kn", "=", 
         RowBox[{"StringLength", "[", "key", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Klist", "=", 
         RowBox[{
          RowBox[{"ToCharacterCode", "[", 
           RowBox[{"ToUpperCase", "[", "key", "]"}], "]"}], "-", "65"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"i", "=", "1"}], ",", 
          RowBox[{"i", "<=", 
           RowBox[{"StringLength", "[", "text", "]"}]}], ",", 
          RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{"For", "[", 
           RowBox[{
            RowBox[{"j", "=", "0"}], ",", 
            RowBox[{"j", "<=", "Kn"}], ",", 
            RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
            RowBox[{"If", "[", 
             RowBox[{
